{
    "repositories": [
       {
            "type": "vcs",
            "url": "https://github.com/wooble/pear-exception"
        }
    ],
    "authors": [
        {
            "email": "dbs@php.net",
            "name": "Dan Scott",
            "role": "Lead",
            "homepage": "https://coffeecode.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "File": "./"
        }
